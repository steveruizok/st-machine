  /* Component styles go here */
}

.light {
  width: 64px;
  height: 64px;
  border-radius: 50%;
  border: 1px solid #ccc;
  margin: 8px;
  -webkit-filter: brightness(60%);
  filter: brightness(60%);
}

.red {
  background: #fe0002;
}

.yellow {
  background: #ffff02;
}

.green {
  background: #3cb54c;
}

.on {
  -webkit-filter: saturate(100);
  filter: saturate(100);
}